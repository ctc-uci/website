    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "prettier": "^3.3.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1"
  },
  "packageManager": "pnpm@9.9.0",
  "engines": {
    "pnpm": "^9.9.0",
    "node": ">=18"
